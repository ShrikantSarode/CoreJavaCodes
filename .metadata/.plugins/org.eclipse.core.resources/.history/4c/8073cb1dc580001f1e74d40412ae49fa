package Assignment6;

abstract public class GeometricShape {

	abstract void area();
	abstract void perimeter();
	
}
class Circle extends GeometricShape{

	@Override
	void area() {
		
		
	}

	@Override
	void perimeter() {
	 
		
	}
	
}
class Rectangle extends GeometricShape{

	@Override
	void area() {
		 
		
	}

	@Override
	void perimeter() {
		 
		
	}
	
}
class Triangle extends GeometricShape{

	@Override
	void area() {
		 
		
	}

	@Override
	void perimeter() {
		 
		
	}
	
}
