package assignment7;

public class Library {
	
	//size=0
	//bookSize=6   ==>final
	//constru=>crud
	//display
	
	Book book;//has a relationship
	int size;
	final int bookSize=6;
	
	public Library(int size,Book book) {
		this.size=size;
		this.book=book;
	}
	
	public void display()
	{
		System.out.println("Book Book Size: "+bookSize);
		System.out.println("Book Book Title: "+book.title+"And Book Author: "+book.author);
	}

}
