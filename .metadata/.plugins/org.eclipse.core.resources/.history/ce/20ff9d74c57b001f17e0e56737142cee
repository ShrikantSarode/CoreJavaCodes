package module2;
import java.lang.Math;

public class MathDemo {

	public static void main(String[] args) {
		
		System.out.println(Math.random());
		System.out.println(Math.random());
		System.out.println(Math.random());
		System.out.println(Math.random());
		System.out.println((int)Math.random() * 100);
	}
}
