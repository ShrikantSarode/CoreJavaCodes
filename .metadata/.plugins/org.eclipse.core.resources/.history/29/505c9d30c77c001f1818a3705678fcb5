package patterns;

public class P11 {
	public static void main(String[] args) {
		
		int row=6;
		int col=1;
		int space=5;
		
		for(int i=0;i<row;i++)
		{
			for(int j=0;j<i;j++)
			{
				for(int k=0;k<space;k++)
				{
					System.out.print("*");
				}
			}
			
			System.out.println();
		}
	}

}
