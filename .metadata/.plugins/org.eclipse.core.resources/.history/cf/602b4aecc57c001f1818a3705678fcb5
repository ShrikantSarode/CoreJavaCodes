package patterns;

public class P1 {

	public static void main(String[] args) {
		
		int row=6;
		int col=6;
		
		for(int i=1;i<row;i++)
		{
			for(int j=1;j<=col;j++)
			{
				System.out.print(" "+j);
			}
			System.out.println();
		}
		System.out.println("=============================");
		int r=6;
		int c=6;
		
		for(int i=1;i<r;i++)
		{
			for(int j=1;j<=c;j++)
			{
				System.out.print(" "+i);
			}
			System.out.println();
		}
	}

}
