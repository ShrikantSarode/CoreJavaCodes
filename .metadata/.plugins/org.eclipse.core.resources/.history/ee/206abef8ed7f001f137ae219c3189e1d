package inheritance;

public class TestPerson1 {

	public static void main(String[] args) {
		
		Person[] arr=new Person[5];
		
		arr[0]= new Person("Shrikant\t", 22, 'M');
		arr[1]= new Person("Ajay\\t", 22, 'M');
		arr[2]= new Person("Dhanya\\t", 22, 'M');
		arr[3]= new Person("Sudharshan\\t", 22, 'M');
		arr[4]= new Person("Ashvini\\t", 21, 'F');
		
		for(int i=0;i<arr.length;i++)
		{
			System.out.println(arr[i].getName()+" is "+arr[i].getAge()+" year old and gender is "+arr[i].getGender());
		}
		
		
		
	}
}
