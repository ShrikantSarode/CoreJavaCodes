	public void display()
	{
		System.out.println("Enter name, age, sal: ");
		name=sc.next();
		age=sc.nextInt();
		sal=sc.nextDouble();
	}
	public void checkAge()
	{
		if(age>18 && age<=100)
		{
			System.out.println("You are Eligible.");
		}
		else {
			System.out.println("You are not Eligible.");
		}
	}
	public void checkSalary()
	{
		
	}
	
}
