package Static;

public class BankAccount {
	
	private int accNo;
	private double bal;
	private static int idNum=0;
	
	
	public BankAccount() {
		accNo=idNum++;
		bal=0;
	}
	
	public BankAccount(double bal) {
		super();
		this.accNo = accNo;
		this.bal = bal;
	}

	public static int getIdNum()
	{
		return idNum;
	}

	public static void main(String[] args) {
		
		System.out.println(idNum);
	}

}
