package patterns;

public class P13 {

	public static void main(String[] args) {
		
		int row=6;
		int space=5;
		int star=1;
		
		for(int i=0;i<row;i++)
		{
			for(int j=0;j<space;j++)
			{
				System.out.print(" ");
			}
			for(int k=0;k<star;k++)
			{
				System.out.print((i++)+1+" ");
			}
			space--;
			star++;
			System.out.println();
		}
	}
}
