	public static void main(String[] args) {
		
		Student s1 = new Student(1,"Shri",95);
		
		s1.display();
		s1.markAttendance();
		System.out.println("===========================");
		Student s2 = new Student();
		s2.display();
		s2.markAttendance();
	}
}
