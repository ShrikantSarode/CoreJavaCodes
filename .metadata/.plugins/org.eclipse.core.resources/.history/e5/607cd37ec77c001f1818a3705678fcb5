package patterns;

public class P11 {
	public static void main(String[] args) {
		
		int row=6;
		int col=6;
		int space=5;
		int star=1;
		
		for(int i=1;i<row;i++)
		{
			for(int j=1;j<i;j++)
			{
				for(int k=0;k<space;k++)
				{
					System.out.print(" ");
				}
				for(int l=0;i<star;l++)
				{
					System.out.println("*");
				}
			}
			
			System.out.println();
		}
	}

}
