package assignment5;

public class Bank {

	private double bal;
	private String name;
	
	public Bank() {
		super();
		bal=0;
		name=null;
	}
	public Bank(double bal, String name) {
		super();
		this.bal = bal;
		this.name = name;
	}
	
	public double getBal() {
		return bal;
	}
	public void setBal(double bal) {
		this.bal = bal;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	@Override
	public String toString() {
		return "Bank [bal=" + bal + ", name=" + name + "]";
	}
	
}
