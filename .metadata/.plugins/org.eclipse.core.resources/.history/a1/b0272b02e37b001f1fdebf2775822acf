package module3;

import java.util.Scanner;

public class IfDemo3 {

	public static void main(String[] args) {
		
		int count=0;
		
		Scanner sc=new Scanner(System.in);
		
		System.out.println("Enter any Number: ");
		int n=sc.nextInt();
		
		while(n!=0)
		{
			n/=10;
			count++;
			
		}

	}

}
