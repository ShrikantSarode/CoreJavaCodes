package assignment5;

public class Student {

	private String address;
	private String name;
	private int age;
	public Student() {
		super();
		name=null;
		age=0;
		address=null;
	}
	public Student(String address, String name, int age) {
		super();
		this.address = address;
		this.name = name;
		this.age = age;
	}
	
	
	
}
