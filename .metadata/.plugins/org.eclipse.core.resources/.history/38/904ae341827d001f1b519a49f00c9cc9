package array;

public class A1 {

	int[] a= {1,2,3,4};
	int max=0;
	
	public void max(int a[])
	{
		for(int i=0;i<a.length;i++)
		{
			if(a[i]>max)
			{
				max=a[i];
			}
		}
		System.out.println("Max: "+max);
	}
	
